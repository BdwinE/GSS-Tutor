: process.env.VUE_APP_USER_ID,\r\n        // userId: '79bdc04b-100c-4513-9549-c0de35a02ff3',\r\n        // userId: '70992af8-81e9-4315-86f9-98a219698bf0',\r\n        error: {}\r\n    },\r\n    mutations: {\r\n        request(state){\r\n            state.status = 'loading'\r\n        },\r\n        success(state, user){\r\n            state.status = 'success'\r\n            state.userId = user.id\r\n        },\r\n        error(state, err){\r\n            state.userId = ''\r\n            state.error = err\r\n        },\r\n        logout(state){\r\n            state.userId = ''\r\n        }\r\n    },\r\n    actions: {\r\n        login({ commit }, user){\r\n            return new Promise((resolve) => {\r\n                commit('request')\r\n                this.authService.loginWithRedirect()\r\n                commit('success', this.authService.user)\r\n                resolve()\r\n            })\r\n        },\r\n        logout({ commit }){\r\n            return new Promise((resolve) => {\r\n                commit('logout')\r\n                this.authService.logout()\r\n                resolve()\r\n            })\r\n        }\r\n    },\r\n    getters: {\r\n        userId: state => state.userId,\r\n        isAuthenticated: () => this.authService.isAuthenticated,\r\n        error: state => state.error\r\n    }\r\n}\r\nexport default auth"]}]}